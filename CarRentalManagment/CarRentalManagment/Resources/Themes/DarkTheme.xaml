<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryColor">#FF9AB8FF</Color>
    <Color x:Key="PrimaryDarkColor">#FF82A2F8</Color>
    <Color x:Key="SecondaryColor">#FF3C3C60</Color>
    <Color x:Key="AccentColor">#FFA0A8FF</Color>
    <Color x:Key="BackgroundColor">#FF151520</Color>
    <Color x:Key="SurfaceColor">#FF1F1F2D</Color>
    <Color x:Key="SurfaceAltColor">#FF2A2A3D</Color>
    <Color x:Key="TextPrimaryColor">#FFF5F5F9</Color>
    <Color x:Key="TextSecondaryColor">#FFC4C4D7</Color>
    <Color x:Key="MenuBackgroundColor">#FF151520</Color>
    <Color x:Key="MenuSeparatorColor">#FF2F2F49</Color>
    <Color x:Key="OverlayColor">#99000000</Color>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}" />
    <SolidColorBrush x:Key="PrimaryDarkBrush" Color="{StaticResource PrimaryDarkColor}" />
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource SecondaryColor}" />
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}" />
    <SolidColorBrush x:Key="PageBackgroundBrush" Color="{StaticResource BackgroundColor}" />
    <SolidColorBrush x:Key="CardBackgroundBrush" Color="{StaticResource SurfaceColor}" />
    <SolidColorBrush x:Key="CardAltBackgroundBrush" Color="{StaticResource SurfaceAltColor}" />
    <SolidColorBrush x:Key="TextPrimaryBrush" Color="{StaticResource TextPrimaryColor}" />
    <SolidColorBrush x:Key="TextSecondaryBrush" Color="{StaticResource TextSecondaryColor}" />
    <SolidColorBrush x:Key="MenuBackgroundBrush" Color="{StaticResource MenuBackgroundColor}" />
    <SolidColorBrush x:Key="MenuSeparatorBrush" Color="{StaticResource MenuSeparatorColor}" />
    <SolidColorBrush x:Key="MenuForegroundBrush" Color="#FFF5F5F9" />
    <SolidColorBrush x:Key="OverlayBrush" Color="{StaticResource OverlayColor}" />
    <SolidColorBrush x:Key="InputBackgroundBrush" Color="#FF2F2F49" />
    <SolidColorBrush x:Key="InputForegroundBrush" Color="#FFF5F5F9" />
    <SolidColorBrush x:Key="BorderBrush" Color="#33F5F5F9" />
    <SolidColorBrush x:Key="SuccessBrush" Color="#FF34A853" />
    <SolidColorBrush x:Key="ErrorBrush" Color="#FFEA4335" />
</ResourceDictionary>
