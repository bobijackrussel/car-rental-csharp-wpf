<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryColor">#FF4E63FF</Color>
    <Color x:Key="PrimaryDarkColor">#FF3D51F0</Color>
    <Color x:Key="SecondaryColor">#FFE2E6FF</Color>
    <Color x:Key="AccentColor">#FF262645</Color>
    <Color x:Key="BackgroundColor">#FFF5F5F9</Color>
    <Color x:Key="SurfaceColor">#FFFFFFFF</Color>
    <Color x:Key="SurfaceAltColor">#FFECECF7</Color>
    <Color x:Key="TextPrimaryColor">#FF262645</Color>
    <Color x:Key="TextSecondaryColor">#FF5A5A7B</Color>
    <Color x:Key="MenuBackgroundColor">#FF262645</Color>
    <Color x:Key="MenuSeparatorColor">#FF3C3C60</Color>
    <Color x:Key="OverlayColor">#66000000</Color>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}" />
    <SolidColorBrush x:Key="PrimaryDarkBrush" Color="{StaticResource PrimaryDarkColor}" />
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource SecondaryColor}" />
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}" />
    <SolidColorBrush x:Key="PageBackgroundBrush" Color="{StaticResource BackgroundColor}" />
    <SolidColorBrush x:Key="CardBackgroundBrush" Color="{StaticResource SurfaceColor}" />
    <SolidColorBrush x:Key="CardAltBackgroundBrush" Color="{StaticResource SurfaceAltColor}" />
    <SolidColorBrush x:Key="TextPrimaryBrush" Color="{StaticResource TextPrimaryColor}" />
    <SolidColorBrush x:Key="TextSecondaryBrush" Color="{StaticResource TextSecondaryColor}" />
    <SolidColorBrush x:Key="MenuBackgroundBrush" Color="{StaticResource MenuBackgroundColor}" />
    <SolidColorBrush x:Key="MenuSeparatorBrush" Color="{StaticResource MenuSeparatorColor}" />
    <SolidColorBrush x:Key="MenuForegroundBrush" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="OverlayBrush" Color="{StaticResource OverlayColor}" />
    <SolidColorBrush x:Key="InputBackgroundBrush" Color="#FFF2F4FF" />
    <SolidColorBrush x:Key="InputForegroundBrush" Color="#FF333333" />
    <SolidColorBrush x:Key="BorderBrush" Color="#1A262645" />
    <SolidColorBrush x:Key="SuccessBrush" Color="#FF0F9D58" />
    <SolidColorBrush x:Key="ErrorBrush" Color="#FFDB4437" />
</ResourceDictionary>
